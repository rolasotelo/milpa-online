{
  "name": "la-milpa-online",
  "version": "1.0.0",
  "description": "la milpa card game web",
  "main": "src/index.js",
  "repository": "git@github.com:rsoteloa/la-milpa-online.git",
  "author": "Rolando Sotelo <rolando.s.alarcon@icloud.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -r dist",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "webpack serve --hot --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "sprite:buttons": "svg-sprite -s --symbol-dest svg-sprite --symbol-sprite buttons-sprite.svg svg-sprite/individual-icons/buttons/*.svg",
    "sprite:banners": "svg-sprite -s --symbol-dest svg-sprite --symbol-sprite banners-sprite.svg svg-sprite/individual-icons/banners/*.svg",
    "sprite:create": "svg-sprite -s --symbol-dest svg-sprite --symbol-sprite create-sprite.svg svg-sprite/individual-icons/create/*.svg",
    "sprite:welcome": "svg-sprite -s --symbol-dest svg-sprite --symbol-sprite welcome-sprite.svg svg-sprite/individual-icons/welcome/*.svg",
    "sprite": "yarn sprite:buttons && yarn sprite:banners && yarn sprite:create && yarn sprite:welcome"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@hot-loader/react-dom": "17.0.1",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/uuid": "8.3.1",
    "autoprefixer": "10.2.6",
    "babel-jest": "27.0.2",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "html-webpack-plugin": "5.3.1",
    "jest": "27.0.4",
    "postcss-loader": "6.1.0",
    "prettier": "2.3.1",
    "react-test-renderer": "17.0.2",
    "style-loader": "2.0.0",
    "svg-sprite": "1.5.1",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "webpack": "5.39.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@headlessui/react": "1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.2.0",
    "socket.io-client": "4.1.2",
    "tailwindcss": "2.2.2",
    "uuid": "8.3.2"
  }
