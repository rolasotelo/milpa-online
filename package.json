{
  "name": "milpa-online",
  "version": "1.0.1",
  "description": "Milpa: online card game",
  "main": "src/index.js",
  "repository": "git@github.com:rolasotelo/milpa-online.git",
  "author": "Rolando Sotelo <rola@hey.com>",
  "keywords": [
    "react",
    "taildwindcss",
    "socket.io",
    "game"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -r dist",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "webpack serve --hot --mode development",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "yarn lint && jest",
    "test:watch": "jest --watch",
    "sprite:buttons": "svg-sprite -s --symbol-dest svgs --symbol-sprite buttons-sprite.svg svgs/individual-icons/buttons/*.svg",
    "sprite:banners": "svg-sprite -s --symbol-dest svgs --symbol-sprite banners-sprite.svg svgs/individual-icons/banners/*.svg",
    "sprite:create": "svg-sprite -s --symbol-dest svgs --symbol-sprite create-sprite.svg svgs/individual-icons/create/*.svg",
    "sprite:welcome": "svg-sprite -s --symbol-dest svgs --symbol-sprite welcome-sprite.svg svgs/individual-icons/welcome/*.svg",
    "sprite:navbar": "svg-sprite -s --symbol-dest svgs --symbol-sprite navbar-sprite.svg svgs/individual-icons/navbar/*.svg",
    "sprite:art": "svg-sprite -s --symbol-dest svgs --symbol-sprite art-sprite.svg svgs/individual-icons/art/*.svg",
    "sprite:leaderboard": "svg-sprite -s --symbol-dest svgs --symbol-sprite leaderboard-sprite.svg svgs/individual-icons/leaderboard/*.svg",
    "sprite": "yarn sprite:buttons && yarn sprite:banners && yarn sprite:create && yarn sprite:welcome"
  },
  "devDependencies": {
    "@babel/core": "~7.14.6",
    "@babel/preset-env": "~7.14.5",
    "@babel/preset-react": "~7.14.5",
    "@hot-loader/react-dom": "~17.0.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "~26.0.23",
    "@types/react": "~17.0.11",
    "@types/react-dom": "~17.0.8",
    "@types/react-router-dom": "~5.1.7",
    "@types/underscore": "^1.11.3",
    "@types/uuid": "~8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "~27.0.2",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "~4.0.0-alpha.0",
    "copy-webpack-plugin": "~9.0.0",
    "css-loader": "~5.2.6",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "generate-changelog": "^1.8.0",
    "html-webpack-plugin": "~5.3.1",
    "jest": "~27.0.4",
    "postcss": "^8.4.14",
    "postcss-loader": "~6.1.0",
    "prettier": "^2.5.1",
    "style-loader": "~2.0.0",
    "svg-sprite": "~1.5.1",
    "ts-jest": "~27.0.3",
    "ts-loader": "~9.2.3",
    "typescript": "~4.3.4",
    "url-loader": "~4.1.1",
    "webpack": "~5.39.1",
    "webpack-bundle-analyzer": "~4.4.2",
    "webpack-cli": "~4.7.2",
    "webpack-dev-server": "~3.11.2"
  },
  "dependencies": {
    "@headlessui/react": "~1.6.3",
    "@popperjs/core": "^2.11.5",
    "@rolasotelo/milpa-ruleset": "^0.1.0",
    "@sentry/react": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "i18next": "^21.8.3",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-hot-loader": "~4.13.0",
    "react-i18next": "^11.16.7",
    "react-popper": "^2.3.0",
    "react-router-dom": "~5.3.3",
    "socket.io-client": "~4.1.2",
    "tailwindcss": "^3.1.3",
    "underscore": "~1.13.3",
    "uuid": "~8.3.2"
  }
}
